{
    "name": "nativescript-mediafilepicker",
    "version": "3.0.2",
    "description": "NativeScript mediafilepicker plugin.",
    "main": "mediafilepicker",
    "typings": "index.d.ts",
    "nativescript": {
        "platforms": {
            "android": "6.0.0",
            "ios": "6.0.1"
        }
    },
    "scripts": {
        "tsc": "npm i && tsc",
        "build": "npm i && npm run tsc && npm run build.native",
        "build.native": "node scripts/build-native.js",
        "tslint": "cd .. && tslint \"**/*.ts\" --config tslint.json --exclude \"**/node_modules/**\" --exclude \"**/typings/**\"",
        "ci.tslint": "npm i && tslint '**/*.ts' --config '../tslint.json' --exclude '**/node_modules/**' --exclude '**/platforms/**' --exclude '**/typings/**'",
        "prepack": "npm run build.native",
        "demo.ios": "npm i && cd ../demo && tns run ios",
        "demo.android": "npm i && cd ../demo && tns run android",
        "demo.reset": "cd ../demo && npx rimraf -- hooks node_modules platforms package-lock.json",
        "plugin.prepare": "npm run build && cd ../demo && tns plugin remove @klippa/nativescript-login && tns plugin add ../src",
        "clean": "npm run demo.reset && npx rimraf -- node_modules package-lock.json && npm i"
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS"
    ],
    "author": {
        "name": "Jibon L. Costa",
        "email": "jiboncosta57@gmail.com"
    },
    "bugs": {
        "url": "https://github.com/jibon57/nativescript-mediafilepicker/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/jibon57/nativescript-mediafilepicker",
    "readmeFilename": "README.md",
    "devDependencies": {
        "prompt": "^1.0.0",
        "rimraf": "^2.6.3",
        "semver": "^5.6.0",
        "tns-core-modules": "^6.4.1",
        "tns-platform-declarations": "^6.4.1",
        "tslint": "^5.12.1",
        "typescript": "~3.4.5"
    },
    "dependencies": {
        "nativescript-permissions": "^1.3.8"
    },
    "bootstrapper": "nativescript-plugin-seed"
}
